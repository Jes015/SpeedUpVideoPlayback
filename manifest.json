{
    "$schema": "https://json.schemastore.org/chrome-manifest",
    "manifest_version": 3,
    "name": "Speed Up Video Playback",
    "version": "0.0.1",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "256": "icon256x256.png",
        "128": "icon128x128.png",
        "48": "icon48x48.png",
        "16": "icon16x16.png"
    }
}